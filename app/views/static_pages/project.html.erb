<script>
function show(link, elem)
{
  if ($(link).hasClass('highlight')) {
    $('.helper').css('display', 'none');
    $('.highlight').removeClass('highlight');
  } else {
    $('.helper').css('display', 'none');
    $('.highlight').removeClass('highlight');
    $(link).addClass('highlight');
    $('#' + elem).css('display', 'inline-block');
  }
}
</script>
