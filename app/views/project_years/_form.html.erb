<%= simple_form_for @project_year, url: project_project_years_path do |f| %>
<div class="content">
  <h1 class="header"> New Project Year </h1>
  <div class="card">
    <h1> Project Details </h1>
    <div class="card-content card-content-form">
      <p>
        <%= f.input :date, label: "Year" %>
        <%= f.hidden_field :project_id %>
      </p>
    </div>
  </div>
</div>
<div class="footer">
  <a class="card">
    <%= f.button :submit %>
  </a>
  <%= link_to project_path(@project_year.project), class: "card" do %>
    <h1 class="h1-alt"> Cancel </h1>
  <% end %>
</div>
<% end %>
